doctype html
html
  head
    title Roster
    include head.pug
    script(src="reactTest.js" type="text/babel")
  body
    div#example
    div.container
      h1=(playerName + '\'s Roster')
      table.table
        tr
          th Position
          th Name
        each card in roster
          tr
            td= card.position
            td= card.name
